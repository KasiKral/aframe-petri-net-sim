{
  "name": "aframe-petri-net-sim",
  "version": "1.0.0",
  "description": "Base for creating virtual reality experiences on the Web using A-Frame. Using CPN tools file to simulate scene logic with Petri nets",
  "main": "server.js",
  "scripts": {
    "start": "npm run development && node server.js",
    "dev": "start node server.js && start npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test"
    }
  },
  "engines": {
    "node": ">=16.6.1"
  },
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "author": "Adam Ka≈°ela",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "bootstrap": "^5.1.3",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "laravel-mix": "^6.0.41",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.1.0"
  }
}
